    "allure-commandline": "^2.13.0",
    "appium": "^1.17.1",
    "chai": "^3.5.0",
    "chai-subset": "^1.6.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "cldr-data": "^32.0.1",
    "codeceptjs": "^2.6.3",
    "codeceptjs-cucumber": "^2.0.15",
    "codeceptjs-saucehelper": "^1.4.0",
    "codeceptjs-saucelabs": "^2.0.10",
    "codeceptjs-shared": "^2.0.8",
    "css-loader": "^3.5.3",
    "debug": "^4.1.1",
    "deepmerge": "^3.3.0",
    "eslint": "^3.19.0",
