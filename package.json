{
  "name": "sfra",
  "version": "5.0.1",
  "description": "Storefront Reference Architecture",
  "main": "index.js",
  "engines": {
    "node": ">=4.0 <12.0.0"
  },
  "scripts": {
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "cover": "sgmf-scripts --cover 'test/unit'",
    "test:integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
    "test:acceptance:custom": "npx codeceptjs run --plugins retryFailedStep --profile",
    "test:acceptance:deep": "npx codeceptjs run --plugins retryFailedStep --grep '(?=.*)^(?!.*@mobile)^(?!.*@tablet)^(?!.*@pageDesigner)' --profile",
    "test:acceptance:smoke": "npx codeceptjs run --plugins retryFailedStep --grep @happyPath --profile",
    "test:acceptance:pagedesigner": "npx codeceptjs run --plugins retryFailedStep --grep @pageDesigner --profile",
    "test:acceptance:desktop": "npx codeceptjs run --plugins retryFailedStep --grep '(?=.*)^(?!.*@mobile)^(?!.*@tablet)^(?!.*@pageDesigner)^(?!.*@deepTest)' --profile",
    "test:acceptance:mobile": "npx codeceptjs run --plugins retryFailedStep --profile sauce:phone --grep @mobile",
    "test:acceptance:tablet": "npx codeceptjs run --plugins retryFailedStep --profile sauce:tablet --grep @tablet",
    "test:acceptance:parallel": "npx codeceptjs run-multiple parallel --plugins retryFailedStep --profile",
    "test:acceptance:multibrowsers": "npx codeceptjs run-multiple multibrowsers --plugins retryFailedStep --profile",
    "test:acceptance:report": "./node_modules/.bin/allure serve test/acceptance/report",
    "bdd:snippets": "./node_modules/.bin/codeceptjs bdd:snippets --path",
    "compile:scss": "node ./bin/Compile-SASS.js --brand=all --env=dev --debug=no --watch=no",
    "compile:scss:prod": "node ./bin/Compile-SASS.js --brand=all --env=prod --debug=no --watch=no",
    "compile:scss:og": "node ./bin/Compile-SASS.js --brand=app_delmonte_core --env=dev --debug=no --watch=no",
    "compile:js": "node ./bin/Compile-JS.js --brand=all --env=dev --debug=no --watch=no",
    "compile:js:prod": "node ./bin/Compile-JS.js --brand=all --env=prod --debug=no --watch=no",
    "compile:js:og": "node ./bin/Compile-JS.js --brand=app_delmonte_core --env=dev --debug=no --watch=no",
    "compile:svg": "node ./bin/Compile-SVG.js --brand=all --debug=no --watch=no",
    "compile:svg:og": "node ./bin/Compile-SVG.js --brand=app_delmonte_core --debug=no --watch=no",
    "compile:fonts": "node bin/Compile-Fonts.js",
    "build:reviews": "cd cartridges && cd int_netreviews_sfra && npm install && npm run build",
    "build": "npm run compile:js;npm run compile:fonts;npm run compile:scss;",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "sgmf-scripts --lint css",
    "lint:js": "sgmf-scripts --lint js",
    "upload": "sgmf-scripts --upload",
    "uploadCartridge": "sgmf-scripts --uploadCartridge app_storefront_base && sgmf-scripts --uploadCartridge modules && sgmf-scripts --uploadCartridge bm_app_storefront_base",
    "watch:scss": "node ./bin/Compile-SASS.js --brand=all --env=dev --debug=no --watch=yes",
    "watch:scss:og": "node ./bin/Compile-SASS.js --brand=app_delmonte_core --env=dev --debug=no --watch=yes",
    "watch:js": "node ./bin/Compile-JS.js --brand=all --env=dev --debug=no --watch=yes",
    "watch:js:og": "node ./bin/Compile-JS.js --brand=app_delmonte_core --env=dev --debug=no --watch=yes",
    "release": "node bin/Makefile release --",
    "siteimport": "cd ./sites/ && node ../bin/Site-Imports.js",
    "siteimport:full:template": "cd ./sites/ && node ../bin/Site-Imports.js --process=full --import=template",
    "siteimport:full:demo": "cd ./sites/ && node ../bin/Site-Imports.js --process=full --import=demo",
    "siteimport:full:setup": "cd ./sites/ && node ../bin/Site-Imports.js --process=full --import=setup",
    "siteimport:full:all": "cd ./sites/ && node ../bin/Site-Imports.js --process=full --import=all",
    "siteimport:update:template": "cd ./sites/ && node ../bin/Site-Imports.js --process=update --import=template",
    "siteimport:update:demo": "cd ./sites/ && node ../bin/Site-Imports.js --process=update --import=demo",
    "siteimport:update:setup": "cd ./sites/ && node ../bin/Site-Imports.js --process=update --import=setup",
    "siteimport:update:all": "cd ./sites/ && node ../bin/Site-Imports.js --process=update --import=all",
    "git-build-stg": "cd build-suite && npm install && grunt dist --project=stg",
    "git-build-dev": "cd build-suite && npm install && grunt dist --project=dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalesforceCommerceCloud/storefront-reference-architecture.git"
  },
  "author": "Ilya Volodin <ivolodin@demandware.com>",
  "brands": [
    "app_delmonte_core",
    "app_storefront_base"
  ],
  "license": "ISC",
  "homepage": "https://github.com/SalesforceCommerceCloud/storefront-reference-architecture",
