    "mocha-junit-reporter": "^1.23.1",
    "moment-timezone": "^0.5.28",
    "node-sass": "^4.14.1",
    "postcss-loader": "^2.1.6",
    "properties-parser": "^0.3.1",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.5",
    "sass": "^1.35.1",
    "sass-loader": "^7.3.1",
    "selenium-standalone": "^6.16.0",
    "sgmf-scripts": "^2.4.1",
    "shelljs": "^0.8.4",
    "should": "^13.2.3",
    "sinon": "^1.17.7",
    "style-loader": "^0.21.0",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.5.0",
    "url": "^0.11.0",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-sauce-service": "^0.4.14",
    "webdriverio": "^5.22.4",
    "webpack": "^5.42.1",
    "xml2js": "^0.4.23"
  },
  "dependencies": {
    "archiver": "^4.0.1",
    "babel-loader": "^8.0.0",
    "bootstrap": "4.3.1",
    "cleave.js": "^1.5.10",
    "flag-icon-css": "^2.9.0",
    "font-awesome": "^4.7.0",
    "grunt": "^1.1.0",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.2.0",
    "md5-file": "^5.0.0",
    "mkdir-recursive": "^0.4.0",
    "node-watch": "^0.6.4",
    "popper.js": "^1.14.8",
    "pretty-log": "^0.1.0",
    "prompt": "^1.0.0",
    "slick-carousel": "^1.8.1",
    "svg-sprite": "^1.5.0",
    "swiper": "^5.3.8",
    "uglifycss": "0.0.29",
    "uglifyjs": "^2.4.11",
    "underscore": "^1.10.2"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "packageName": "app_storefront_base",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
